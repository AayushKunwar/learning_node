extends layouts/main-layout.pug 


block content 
    main
        if prods.length > 0
            .grid
                for product in prods
                    article.card.product-item
                        header.card__header 
                            h1.product__title #{product.title}
                        .card__image
                            img(src="", alt="Photo here")   
        else
            h1 No Products